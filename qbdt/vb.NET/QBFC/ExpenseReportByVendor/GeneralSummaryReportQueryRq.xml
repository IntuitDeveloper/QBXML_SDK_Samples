<!-- Summary: Sample illustrating a general summary report.            -->
<!--                                                                   -->
<!-- Description:                                                      -->
<!-- Queries a General summary report with type ExpenseByVendorSummary -->
<!-- Sumarizes columns by month.                                       -->
<!-- Uses the fiscal year for the reporting calendar                   -->
<!-- ================================================================= -->
<!--                                                                   -->
<!-- Language: VB.NET                                                   -->
<!-- Structure: QBFC                                                   -->
<!-- Deployment: any                                                   -->
<!-- QuickBooks: any                                                   -->
<!-- ================================================================= -->
<?qbxml version="2.0"?>
<QBXML>
<QBXMLMsgsRq onError = "continueOnError">
<GeneralSummaryReportQueryRq requestID = "0">
<GeneralSummaryReportType>ExpenseByVendorSummary</GeneralSummaryReportType>
<ReportDateMacro>ThisYearToDate</ReportDateMacro>
<SummarizeColumnsBy>Month</SummarizeColumnsBy>
<ReportCalendar>FiscalYear</ReportCalendar>
</GeneralSummaryReportQueryRq>
</QBXMLMsgsRq>
</QBXML>
